  author = {Li, Da and Liu, Peng and Zhang, Zhaosheng and Zhang, Lei and Deng, Junjun and Wang, Zhenpo and Dorrell, David G. and Li, Weihan and Sauer, Dirk Uwe},
  date = {2022-07},
