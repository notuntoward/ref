  author = {Shi, Yuanyuan and Xu, Bolun and Tan, Yushi and Kirschen, Daniel and Zhang, Baosen},
  date = {2019-06},
  journaltitle = {IEEE Transactions on Automatic Control},
  volume = {64},
  number = {6},
