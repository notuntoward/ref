  url       = {http://www.mth.kcl.ac.uk/~psollich/publications/node27.html},
}

@Article{Aghagolzadeh07hierClustMutInf,
  author    = {Aghagolzadeh, M. and Soltanian-Zadeh, H. and Araabi, B. and Aghagolzadeh, A.},
