@Article{Rowe07fMRIangularRgrsn,
  author    = {Daniel B. Rowe and Christopher P. Meller and Raymond G. Hoffmann},
  title     = {Characterizing phase-only fMRI data with an angular regression model},
  year      = {2007},
  volume    = {161},
