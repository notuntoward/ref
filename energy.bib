@Article{Schmid13betaBoostRgrssn,
  author    = {Schmid, Matthias and Wickler, Florian and Maloney, Kelly O and Mitchell, Richard and Fenske, Nora and Mayr, Andreas},
  title     = {Boosted beta regression},
