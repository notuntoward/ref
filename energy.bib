@TechReport{Sloughter08probWindEnsBayesModAvg,
  author      = {J. McLean Sloughter and Tilmann Gneiting and Adrian E. Raftery},
  title       = {Probabilistic Wind Speed Forecasting using Ensembles and {Bayes}ian Model Averaging},
