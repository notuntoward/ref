@Article{Demsar13spatialPCA,
  author    = {Dem{\v{s}}ar, Ur{\v{s}}ka and Harris, Paul and Brunsdon, Chris and Fotheringham, A Stewart and McLoone, Sean},
  title     = {Principal component analysis on spatial data: an overview},
