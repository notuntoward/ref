@InProceedings{Moller13windFrcstSDE,
  author    = {Jan Kloppenborg M{\o}ller and Pierre Pinson and Henrik Madsen},
  title     = {Probabilistic Forecasts of Wind Power Generation by Stochastic Differential Equation Models},
