  groups    = {Ensemble, PointDerived, doReadNonWPV_2},
  owner     = {sotterson},
  publisher = {Institute of Mathematical Statistics},
  timestamp = {2013.10.04},
  url       = {http://arxiv.org/abs/1202.3819},
}

