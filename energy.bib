@Article{GomezHerrero15cplDynTSens,
  author    = {G{\'o}mez-Herrero, Germ{\'a}n and Wu, Wei and Rutanen, Kalle and Soriano, Miguel C and Pipa, Gordon and Vicente, Raul},
