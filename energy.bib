  institution = {Oregon State University},
  owner       = {sotterson},
  timestamp   = {2013.10.16},
  url         = {http://hdl.handle.net/1957/30896},
}

@InBook{Shinomoto10EstFireRatePoisson,
  pages     = {21--35},
  title     = {Estimating the Firing Rate},
  publisher = {Springer US},
