@misc{Agarwal22MultivariateSingularSpectrum,
  title = {On {{Multivariate Singular Spectrum Analysis}} and Its {{Variants}}},
  author = {Agarwal, Anish and Alomar, Abdullah and Shah, Devavrat},
  year = {2022},
