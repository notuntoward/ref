@misc{Tang23PyEPOPyTorchbasedEndtoEnd,
  title = {{{PyEPO}}: {{A PyTorch-based End-to-End Predict-then-Optimize Library}} for {{Linear}} and {{Integer Programming}}},
  shorttitle = {{{PyEPO}}},
